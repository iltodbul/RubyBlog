<h1>Articles</h1>

<ul class="list-group">
  <% @articles.each do |article| %>
    <li class="list-group-item list-group-item-action">
      <%= link_to article.title, article %>
    </li>
  <% end %>
</ul>

<div class="mb-3">
  <%= link_to "New Article", new_article_path, class: "btn btn-primary" %>
</div>
